# Sonic Labs Network Deployment Configuration
# Copy this to .env and fill in your values

# Private key for deployment (keep secret!)
PRIVATE_KEY=your_private_key_here

# Optional: Sonic Labs API key for contract verification
SONICSCAN_API_KEY=your_soniclabs_api_key_here

# Network configuration
SONIC_TESTNET_RPC=https://rpc.testnet.soniclabs.com
SONIC_MAINNET_RPC=https://rpc.soniclabs.com

# Gas settings
GAS_PRICE=20000000000
GAS_LIMIT=6000000 